#cmd regkey manual add

reg add "HKLM\SOFTWARE\Company\Autopilot" /v MSOffice /t REG_DWORD /d 1 /f

#PS reg key manual add


# Create the key if it doesn't exist
New-Item -Path "HKLM:\SOFTWARE\Company\Autopilot" -Force | Out-Null

# Create DWORD value MSOffice = 1
New-ItemProperty -Path "HKLM:\SOFTWARE\Company\Autopilot" -Name "MSOffice" -Value 1 -PropertyType DWord -Force | Out-Null
